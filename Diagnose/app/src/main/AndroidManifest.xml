<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hyperexchange.diagnose">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_android"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FlashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".NetworkCheckActivity" />
        <activity android:name=".VolumeButtonCheck" />
        <activity android:name=".CheckCamera" />
        <activity android:name=".CameraPreview" />
        <activity android:name=".SingleTouch" />
        <activity android:name=".GoTOCanVasActivity" />
        <activity android:name=".CheckMultiTouch" />
        <activity android:name=".MultiTouchCover" />
        <activity android:name=".Memory" />
        <activity android:name=".LightSensor" />
        <activity android:name=".Accelerometer" />
        <activity android:name=".AccCover" />
        <activity android:name=".ProximitySensor" />
        <activity android:name=".Pedometer" />
        <activity android:name=".CompassActivity" />
        <activity android:name=".HeadSetJackCheck"></activity>
    </application>

</manifest>